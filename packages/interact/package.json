{
  "name": "interact",
  "version": "0.0.1",
  "type": "module",
  "description": "More than just chat: Interactive AI design patterns",
  "main": "dist/interact.cjs",
  "module": "dist/interact.es.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/interact.es.mjs",
      "require": "./dist/interact.cjs"
    },
    "./styles.css": "./dist/styles.css"
  },
  "sideEffects": false,
  "scripts": {
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "vite build",
    "build:css": "tailwindcss -i ./tailwind.css -o ./dist/styles.css --minify",
    "build": "pnpm build:types && pnpm build:js && pnpm build:css"
  },
  "peerDependencies": {
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  },
  "devDependencies": {
    "@tailwindcss/cli": "^4.1.17",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "tailwindcss": "^4",
    "typescript": "^5.9.3",
    "vite": "^7.2.4"
  },
  "dependencies": {
    "clsx": "^2.1.1"
  }
}
